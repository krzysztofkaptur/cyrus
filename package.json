  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "db:generate": "npx drizzle-kit generate --config=./src/drizzle.config.ts",
    "db:migrate": "npx drizzle-kit migrate --config=./src/drizzle.config.ts",
    "db:seed": "node -r esbuild-register src/libs/db/seeds/index.ts",
    "db:ui": "npx drizzle-kit studio --config=./src/drizzle.config.ts",
    "tailwind:css": "npx tailwindcss -i ./src/css/style.css -o ./public/style.css --watch",
    "clean": "rimraf dist",
    "test": "jest --detectOpenHandles --watch"
  },
